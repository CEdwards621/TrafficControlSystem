<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="376.0" prefWidth="493.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.SystemUIController">
   <center>
      <ImageView fx:id="imageOutputImgvw" fitHeight="280.0" fitWidth="454.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <VBox alignment="TOP_CENTER" prefHeight="94.0" prefWidth="493.0" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="feedListCbx" prefHeight="27.0" prefWidth="328.0" promptText="Select A Video Feed" />
            <Button fx:id="selFeedBtn" mnemonicParsing="false" onAction="#startFeed" text="Select Feed" />
         </children>
      </VBox>
   </bottom>
</BorderPane>
